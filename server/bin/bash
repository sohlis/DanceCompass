#!/bin/bash

while true
do
curl -i \
            -H "Accept: application/json" \
         33   -H "Content-Type: application/json" \
                 -X POST -d '{"intensity":".2", "stage":"Rock"}' \
                 http://localhost:3000/api
curl -i \
            -H "Accept: application/json" \
         33   -H "Content-Type: application/json" \
                 -X POST -d '{"intensity":".5", "stage":"Hip-hop"}' \
                 http://localhost:3000/api
curl -i \
            -H "Accept: application/json" \
         33   -H "Content-Type: application/json" \
                 -X POST -d '{"intensity":".9", "stage":"Rock"}' \
                 http://localhost:3000/api
curl -i \
            -H "Accept: application/json" \
         33   -H "Content-Type: application/json" \
                 -X POST -d '{"intensity":"5.7", "stage":"Hip-hop"}' \
                 http://localhost:3000/api
curl -i \
            -H "Accept: application/json" \
         33   -H "Content-Type: application/json" \
                 -X POST -d '{"intensity":"7.2", "stage":"Rock"}' \
                 http://localhost:3000/api
curl -i \
            -H "Accept: application/json" \
         33   -H "Content-Type: application/json" \
                 -X POST -d '{"intensity":"3.5", "stage":"Hip-hop"}' \
                 http://localhost:3000/api
curl -i \
            -H "Accept: application/json" \
         33   -H "Content-Type: application/json" \
                 -X POST -d '{"intensity":"1.9", "stage":"Rock"}' \
                 http://localhost:3000/api
curl -i \
            -H "Accept: application/json" \
         33   -H "Content-Type: application/json" \
                 -X POST -d '{"intensity":"7.7", "stage":"Hip-hop"}' \
                 http://localhost:3000/api
done
